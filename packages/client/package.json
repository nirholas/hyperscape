{
  "name": "@hyperscape/client",
  "version": "0.13.0",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/HyperscapeAI/hyperscape#readme",
  "bugs": {
    "url": "https://github.com/HyperscapeAI/hyperscape/issues"
  },
  "keywords": [
    "hyperscape",
    "3d",
    "components",
    "virtual-worlds",
    "metaverse",
    "three.js",
    "webgpu",
    "real-time"
  ],
  "scripts": {
    "dev": "vite --host --port 3333",
    "build": "cd ../.. && turbo run build --filter=@hyperscape/shared && cd packages/client && NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "build:cf": "NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "preview": "vite preview",
    "lint": "eslint src --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "deploy": "node scripts/deploy-cloudflare.mjs",
    "deploy:prod": "node scripts/deploy-cloudflare.mjs --production"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@farcaster/miniapp-sdk": "^0.2.1",
    "@hyperscape/physx-js-webidl": "workspace:*",
    "@hyperscape/shared": "workspace:*",
