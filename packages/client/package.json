    "@solana/web3.js": "^1.98.4",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-opener": "^2.3.0",
    "@types/styled-components": "^5.1.34",
    "buffer": "^6.0.3",
    "d3": "^7.9.0",
    "eventemitter3": "^5.0.1",
    "hls.js": "^1.6.1",
    "hs-kit": "workspace:*",
    "livekit-client": "^2.9.9",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.554.0",
