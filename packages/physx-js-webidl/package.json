{
  "name": "@hyperscape/physx-js-webidl",
  "version": "2.6.1",
  "description": "Node.js-compatible Javascript/WASM bindings for Nvidia PhysX 5.6.0",
  "author": "fabmax",
  "main": "dist/physx-js-webidl.js",
  "types": "dist/physx-js-webidl.d.ts",
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "bun scripts/copy-prebuilt.mjs",
    "build": "bun scripts/copy-prebuilt.mjs",
    "build:force": "bash ./make.sh",
    "build:debug": "bash ./make.sh debug",
    "build:profile": "bash ./make.sh profile",
    "build:docker": "docker compose run --rm builder bash ./make.sh",
    "generate": "bash ./make.sh",
    "generate:web": "bash ./make.sh",
    "clean": "rm -rf PhysX/physx/compiler/emscripten-* && rm -f dist/physx-js-webidl.*"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/physx-js-webidl.d.ts",
      "import": "./dist/physx-js-webidl.js",
      "require": "./dist/physx-js-webidl.js"
    },
    "./dist/*": "./dist/*"
  }
}
