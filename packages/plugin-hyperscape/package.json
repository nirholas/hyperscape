{
  "name": "@hyperscape/plugin-hyperscape",
  "description": "ElizaOS plugin for Hyperscape - AI agents as real players in 3D multiplayer RPG",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Hyperscape Team",
  "keywords": [
    "elizaos",
    "plugin",
    "hyperscape",
    "ai-agents",
    "multiplayer",
    "rpg",
    "3d"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "@elizaos/core": "^1.6.1",
    "@elizaos/plugin-anthropic": "^1.5.12",
    "@elizaos/plugin-openai": "1.5.16",
    "@elizaos/plugin-openrouter": "^1.5.15",
    "@elizaos/plugin-sql": "^1.6.4",
    "@elizaos/server": "^1.2.9",
    "@hyperscape/shared": "workspace:*",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.525.0",
    "msgpackr": "^1.11.2",
    "react": "^19.2.0",
    "tailwind-merge": "^2.5.4",
    "three": "^0.180.0",
    "ws": "^8.18.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@elizaos/cli": "^1.6.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.8.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.5.10",
    "playwright": "^1.54.2",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun test src/",
    "test:unit": "bun test src/",
    "test:mcp": "bun run scripts/test-mcp.ts",
    "test:eliza": "bun run scripts/test-eliza-agent.ts",
    "test:a2a": "bun run scripts/test-a2a.ts",
    "test:production": "bun run scripts/test-production.ts",
    "test:e2e": "bun run scripts/test-mcp.ts && bun run scripts/test-eliza-agent.ts",
    "test:all": "bun run test:unit && bun run test:e2e && bun run test:production",
    "lint": "eslint src --fix"
  }
}
