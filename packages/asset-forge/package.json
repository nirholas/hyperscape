{
  "name": "asset-forge",
  "version": "1.0.0",
  "description": "AI-powered 3D asset generation system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "concurrently \"npm run dev:api\" \"npm run dev:images\"",
    "dev:api": "npm run build:services && node server/api.mjs",
    "dev:images": "node scripts/start-image-server.mjs",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "npm run clean && vite build",
    "build:services": "node scripts/build-services.mjs",
    "start": "npm run start:backend",
    "start:backend": "concurrently \"npm run start:api\" \"npm run start:image-server\"",
    "start:api": "npm run build:services && node server/api.mjs",
    "start:image-server": "node scripts/start-image-server.mjs",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "preview": "vite preview",
    "assets:audit": "npx tsx scripts/audit-assets.ts",
    "assets:normalize": "npx tsx scripts/normalize-all-assets.ts",
    "assets:extract-tpose": "npx tsx scripts/extract-tpose-simple-cli.ts",
    "assets:check-coverage": "node scripts/check-asset-coverage.mjs",
    "assets:generate-missing": "node scripts/generate-missing-assets.mjs",
    "assets:rebuild-manifests": "node scripts/rebuild-all-manifests.mjs",
    "assets:validate": "node scripts/validate-integration.mjs",
    "check:deps": "depcheck",
    "check:all": "knip",
    "count:lines": "node scripts/count-lines.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "ai",
    "3d",
    "generation",
    "meshy",
    "gpt-4",
    "rpg",
    "game-assets"
  ],
  "author": "Lucid",
  "license": "MIT",
  "dependencies": {
    "@mediapipe/hands": "^0.4.1675469240",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^8.16.8",
    "@tensorflow-models/hand-pose-detection": "^2.0.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "chalk": "^5.6.2",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "immer": "^10.1.1",
    "lucide-react": "^0.545.0",
    "node-fetch": "^3.3.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.180.0",
    "three-mesh-bvh": "^0.9.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^24.8.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.16",
    "depcheck": "^1.4.7",
    "knip": "^5.62.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.3.3",
    "vite": "^7.1.10"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
